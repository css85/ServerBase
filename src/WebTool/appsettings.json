{
  "SessionSettings": {
    "PingCheckDelay": "0.00:01:00",
    "SessionLogDelay": "0.00:00:15",
    "PingRequiredTime": "0.00:05:00",
    "PingTimeout": "0.00:10:00",
    "EnableTracePacketLog": false,
    "SlowPacketTime": "0.00:00:00.5"
  },
  "ServerSessionSettings": {
    "PingCheckDelay": "0.00:00:01",
    "SessionLogDelay": "0.00:00:15",
    "PingRequiredTime": "0.00:00:05",
    "PingTimeout": "0.00:00:05",
    "EnableTracePacketLog": false,
    "SlowPacketTime": "0.00:00:00.5"
  },
  "AppMonitorSettings": {
    "CollectMetricsInterval": "00:00:20"
  },
  "WebToolAppSettings": {
    "AppGroupId": 1,
    "AppGroupName": "default",
    "AppId": 20901,
    "AppName": "webtool-default",
    "ExternalHost": "127.0.0.1",
    "ExternalWebPort": 5000,
    "InternalHost": "127.0.0.1",
    "InternalPort": 20901,
    "ListenHost": "0.0.0.0",
    "ListenWebPort": 5000,
    "ListenInternalPort": 20901
  },
  "CsvSettings": {
    "CsvPath": "../csvdata"
  },
  
  "LanguageSettings": {
    "LanguageTargetType": "Korea"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Expressions"
    ],
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:hh:mm:ss.ff} [{Level:u3}] {Message}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": {
          "maximumDestructuringDepth": 4
        }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": {
          "maximumStringLength": 100
        }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": {
          "maximumCollectionCount": 10
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting": "Debug",
        "Bluegames.Common": "Debug",
        "Shared.Session.Base": "Debug",
        "Shared.Session.Features": "Debug",
        "MySqlConnector": "Warning",
        "Elastic": "Warning"
      }
    }
  }
}
