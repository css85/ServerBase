{
  "DatabaseSettings": {
    "DatabaseProviderType": "LocalDB",
    "GateConnectionStrings": [
      "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=audition_m_testing.gate;Trusted_Connection=True;"
    ],
    "ExchangeConnectionStrings": [
      "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=audition_m_testing.app;Trusted_Connection=True;"
    ],
    "AccountConnectionStrings": [
      "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=audition_m_testing.account;Trusted_Connection=True;"
    ],
    "UserConnectionStrings": [
      "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=audition_m_testing.user;Trusted_Connection=True;"
    ],
    "WalletConnectionStrings": [
      "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=audition_m_testing.wallet;Trusted_Connection=True;"
    ],
    "RedisAdmin": "localhost:6379,abortConnect=false,allowAdmin=true",
    "RedisAdminHostAndPort": "localhost:6379"
  },
  "RedisConfiguration": {
    "AbortOnConnectFail": false,
    "Password": "my_super_secret_password",
    "AllowAdmin": false,
    "Ssl": false,
    "ConnectTimeout": 10000,
    "SyncTimeout": 10000,
    "ConnectRetry": 2,
    "Database": 0,
    // "ServiceName": "redis", // In case you are using Sentinel
    "Hosts": [
      {
        "Host": "localhost",
        "Port": "6379"
      }
    ],
    "ServerEnumerationStrategy": {
      "Mode": "All",
      "TargetRole": "Any",
      "UnreachableServerAction": "Throw"
    },
    "MaxValueLength": 1024,
    "PoolSize": 10,
    "KeyPrefix": "_"
  },
  "SessionSettings": {
    "EnableTracePacketLog": false,
    "SlowPacketTime": "0.00:00:00.5",
    "PingCheckDelay": "0.01:01:00",
    "PingRequiredTime": "0.01:05:00",
    "PingTimeout": "0.01:10:00"
  },
  "ServerSessionSettings": {
    "EnableTracePacketLog": false,
    "SlowPacketTime": "0.00:00:00.5",
    "PingCheckDelay": "0.01:01:00",
    "PingRequiredTime": "0.01:05:00",
    "PingTimeout": "0.01:10:00"
  },
  "GameRuleSettings": {
    "ApUpdateMaxLimit": 100,
    "ApUpdateFrequencyTime": "00:01:00",
    "UserCommentLengthLimit": 50,
    "UserCommentNewLineLimit": 2,
    "TodayUserReportLimit": 5,
    "TodaySameUserReportLimit": 1,
    "ProfileImageSizeLimit": 524288,
    "UserReportReasonLengthLimit": 100,
    "FriendMaxCount": 50,
    "FriendRequestMaxCount": 30,
    "FriendSearchCount": 10,
    "FriendFavoriteMaxCount": 10,
    "GameRoomNoStartMasterKickTimeout": "00:00:15",
    "IngameStartUserKickTimeout": "00:01:00",
    "IngameEndUserKickTimeout": "00:00:20",
    "IngameJoinElapsedLimit": "00:00:10",
    "IngameJoinRemainLimit": "00:00:10",
    "IngameKeepAliveInterval": "00:00:01",
    "CoupleRequestLimit": "1.00:00:00",
    "ShowDancerFamUserUseDelay": "00:05:00",
    "IsCheckShowDancerPlayTime": false,
    "DailyPackageRewardInitTime": "04:00:00",
    "EventRankingMainRankerCount": 100
  },
  "TokenSettings": {
    "Secret": "mysupers3cr3tsharedkey!",
    "TokenExpires": "7.00:00:00"
  },
  "ClockSettings": {
    "Offset": "00:00:00"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],

    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:hh:mm:ss.ff} [{Level:u3}] ({ThreadId:D2}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "MinimumLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
